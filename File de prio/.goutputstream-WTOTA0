#include <stdio.h>
#include <stdlib.h>

/* Une file : on insère au début et on retire à la fin */

struct paire {
	int id;
	int prio;
};
typedef struct paire Paire;

struct file {
	
};
typedef struct file File;

File creerFile();
int fileVide(File);
void enfile(File, Paire);
Paire defile(File);
void afficheFile(File);
File libereFile(File);

